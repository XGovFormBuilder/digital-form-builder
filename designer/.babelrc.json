{
  "presets": [
    ["@babel/preset-env",
      {
        "targets": {
          "node": "12"
        }
      }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime"
  ],
  "ignore": ["./node_modules", "../node_modules", "**/node_modules/**"]
}
