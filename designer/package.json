    "@babel/preset-env": "7.10.3",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.3",
    "@babel/runtime": "7.10.3",
    "@cautionyourblast/rollup-plugin-node-builtins": "^2.1.3",
    "@hapi/code": "^8.0.1",
