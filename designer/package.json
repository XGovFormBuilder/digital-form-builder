{
  "name": "digital-form-builder-designer",
  "version": "1.0.0",
  "description": "Cross government digital form builder (designer)",
  "main": "server/index.js",
  "scripts": {
    "lint": "standard",
    "fix-lint": "standard --fix",
    "build": "rollup -c && yarn babel-build",
    "watch": "(rollup -c -w) & yarn babel-watch",
    "dev": "rollup -c -w & node server/index.js",
    "start": "node dist/index.js",
    "symlink-env": "./bin/symlink-config",
    "test-lab": "lab -T node_modules/@xgovformbuilder/lab-babel test/.setup.js test/*.test.js -S -v -r console -o stdout -r html -o unit-test.html -I version -l",
    "test": "yarn flow && yarn lint && yarn test-lab",
    "audit": "npm audit --audit-level=high",
    "postinstall": "flow-typed install -o --ignoreDeps dev",
    "babel-build": "babel server -d dist --copy-files -s inline",
    "babel-watch": "babel server -d dist --copy-files -s inline --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XGovFormBuilder/digital-form-builder/tree/master/designer"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@hapi/hapi": "19.1.1",
    "@hapi/joi": "17.1.1",
    "@hapi/wreck": "17.0.0",
    "digital-form-builder-model": "file:../model",
    "aws-sdk": "^2.723.0",
    "flat": "5.0.0",
    "hapi-pino": "^8.0.1",
    "moment-timezone": "^0.5.31",
    "schmervice": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "7.10.3",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-amd": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.3",
    "@babel/runtime": "7.10.3",
    "@hapi/code": "^8.0.1",
    "@hapi/inert": "^6.0.1",
    "@hapi/lab": "^22.0.4",
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@xgovformbuilder/lab-babel": "2.1.0",
    "acorn": "7.2.0",
    "babel-eslint": "11.0.0-beta.2",
    "dagre": "0.8.2",
    "dotenv": "8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "flow-bin": "^0.130.0",
    "flow-typed": "^3.2.1",
    "govuk-frontend": "3.6.0",
    "jsdom": "^16.2.2",
    "nunjucks": "3.2.1",
    "prismjs": "1.15.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-simple-code-editor": "0.6.1",
    "react-sortable-hoc": "0.8.3",
    "rollup": "2.15.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-globals": "1.4.0",
    "shortid": "^2.2.15",
    "sinon": "^9.0.2",
    "standard": "12.0.1",
    "vision": "5.4.3"
  },
  "standard": {
    "ignore": [
      "dist/",
      "flow-typed"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "eslint": "babel-eslint"
}
