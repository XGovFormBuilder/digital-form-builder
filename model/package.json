{
  "name": "@xgovformbuilder/model",
  "version": "2.0.0",
  "description": "A hapi plugin providing the model for XGov digital form builder based applications",
  "main": "index.js",
  "module": "src/lib.js",
  "scripts": {
    "fix-lint": "eslint --fix",
    "lint": "yarn run eslint ./ -c ../.eslintrc.json",
    "build": "babel --root-mode upward src -d lib --source-maps inline",
    "watch": "bash -c 'nohup yarn run babel --root-mode upward src -d lib --source-maps inline'",
    "test-lab": "lab -T test/.transform.js -S -v -P 'test' test -r console -o stdout -r html -o unit-test.html -I version",
    "test": "yarn flow && yarn lint && yarn test-lab"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XGovFormBuilder/digital-form-builder/tree/master/model"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@babel/eslint-plugin": "^7.11.3",
    "@babel/runtime": "7.10.4",
    "@hapi/hoek": "^9.0.4",
    "@hapi/joi": "17.1.1",
    "boom": "7.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "expr-eval": "2.0.2",
    "govuk-frontend": "3.6.0",
    "hmpo-components": "2.6.5",
    "joi": "14.3.1",
    "moment": "2.24.0",
    "nunjucks": "3.2.1",
    "path": "0.12.7",
    "wreck": "14.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.4",
    "@hapi/code": "^8.0.1",
    "@hapi/lab": "^22.0.4",
    "@xgovformbuilder/lab-babel": "2.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "flow-bin": "^0.130.0",
    "flow-mono-cli": "^1.5.3",
    "flow-typed": "^3.2.1"
  }
}
