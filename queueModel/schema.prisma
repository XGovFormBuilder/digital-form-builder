datasource db {
  provider = "mysql"
  url = env("QUEUE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Submission {
  id Int @id @default(autoincrement())
  webhook_url String?
  created_at DateTime
  updated_at DateTime
  data String @db.VarChar(8192)
  error String?
  return_reference String?
  complete Boolean
  retry_counter Int
}