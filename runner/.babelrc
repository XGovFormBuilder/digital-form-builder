{
  "presets": [
    ["@babel/preset-flow"],
    ["@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "node": "12"
        }
      }
    ],
    "@babel/typescript"
  ],
  "exclude": ["node_modules/**"],
  "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-private-methods"]
}
